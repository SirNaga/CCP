            "user_data": "#!/bin/bash\r\n\r\nset -e\r\nsudo apt update\r\n\r\n#install prometheus and setup files\r\nsudo apt-get -y install prometheus\r\nsudo echo \"[\r\n  {\r\n    \"targets\": [ \"localhost:9100\" ]\r\n  }\r\n]\" \u003e /etc/prometheus/targets.json;\r\nsudo echo \"global:\r\n  scrape_interval: 15s\r\nscrape_configs:\r\n  - job_name: 'prometheus'\r\n    scrape_interval: 5s\r\n    static_configs:\r\n      - targets: ['localhost:9090']\r\n  - job_name: Monitoring Server Node Exporter\r\n    static_configs:\r\n      - targets:\r\n          - 'localhost:9100'\r\n  - job_name: Service Discovery\r\n    file_sd_configs:\r\n      - files:\r\n          - /etc/prometheus/targets.json\r\n        refresh_interval: 10s\" \u003e /etc/prometheus/prometheus.yml;\r\nsudo systemctl restart prometheus\r\n#END install prometheus and setup files\r\n\r\ncurl -fsSL https://get.docker.com -o get-docker.sh\r\nsudo sh get-docker.sh\r\n\r\ngit clone https://github.com/SirRacuga/CCP.git\r\ncd CCP/servicediscovery\r\ndocker build --tag servicediscovery:1.0 .\r\n\r\n#sudo docker pull SirRacuga/prometheus_service_discovery:latest\r\nsudo docker run \\\r\n    -d \\\r\n    -e EXOSCALE_KEY=EXO58ddfe98552a6059aaaad98c \\\r\n    -e EXOSCALE_SECRET=knAP0mmFsKKedu4PmU4yg5mcQ0OxsUfgwOlCuJkJk5Y \\\r\n    -e EXOSCALE_ZONE=at-vie-1 \\\r\n    -e EXOSCALE_INSTANCEPOOL_ID=1ec91256-39af-6354-fa79-3d17428fd5b1 \\\r\n    -e TARGET_PORT=9100 \\\r\n    -v /etc/prometheus:/prometheus \\\r\n    servicediscovery:1.0\r\n",
